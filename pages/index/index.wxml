<!-- 首页 -->
<view class="container flex col fl-align">
  <!-- 置顶搜索栏 -->
  <view class="search-line flex fl-align f28 b-F1">
    <view class="locals flex fl-align c-ff">
      <text>地点</text>
      <image></image>
    </view>
    <view class="search-box flex fl-align b-ff b_r200">
      <icon type="search" size="28" color="#C1C1C1" />
      <text class="c-C1">搜索</text>
    </view>
  </view>
  <!-- 轮播图 -->
  <swiper class="banner" indicator-dots="true" autoplay="true" indicator-active-color="#fff">
    <block wx:for="{{sw_list}}" wx:key="index">
      <swiper-item>
        <image src="../../resources/1.png" class="sw_img"></image>
      </swiper-item>
    </block>
  </swiper>
  <!-- 导航 -->
  <view class="nav-line flex fl-align fl-jsa">

    <view class="nav-line-item">
      <image src="icon/group.png"></image>
    </view>


  </view>
</view>