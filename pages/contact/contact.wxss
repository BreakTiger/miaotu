page {
  background: #f5f5f5;
}

.line {
  padding: 20rpx;
  border-bottom: 1rpx solid #f5f5f5;
  background: #fff;
}

.line>input, .line>view {
  width: 75%;
}

.add {
  margin: 35rpx auto;
  width: 400rpx;
  height: 76rpx;
  line-height: 76rpx;
  border-radius: 40rpx;
  background: linear-gradient(270deg, rgba(65, 216, 249, 1) 0%, rgba(31, 175, 241, 1) 100%);
}

.del {
  margin: 20rpx auto;
  width: 400rpx;
  height: 76rpx;
  line-height: 76rpx;
  border-radius: 40rpx;
  background: #fff;
}
