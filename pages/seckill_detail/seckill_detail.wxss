@import '../../wxParse/wxParse.wxss';

page {
  background: #f5f5f5;
}

.top-line {
  width: 100%;
  height: 88rpx;
  position: fixed;
  top: 0;
  z-index: 10;
}

.goods_detail {
  width: 100%;
  margin-top: 88rpx;
  margin-bottom: 8rpx;
}

.good-imgs {
  width: 100%;
  height: 610rpx;
}

.ims {
  width: 100%;
  height: 100%;
}

.c-45 {
  color: #494445;
}

.good-title {
  width: 670rpx;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
}

.goods-lines {
  width: 670rpx;
  margin-bottom: 26rpx;
}

.hint {
  width: 670rpx;
  margin-bottom: 14rpx;
}

.hint>text:first-child {
  display: inline-block;
  width: 120rpx;
  height: 32rpx;
  line-height: 32rpx;
  border: 1rpx solid rgba(255, 63, 83, 1);
  margin-right: 20rpx;
}

.hint-line {
  width: 100%;
  height: 108rpx;
  background: #fff7ce;
  margin-bottom: 8rpx;
}

.hint-line>image {
  width: 32rpx;
  height: 32rpx;
}

.hint-line>view:nth-child(2) {
  width: 40rpx;
  height: 56rpx;
}

.hint-line>view:last-child {
  width: 560rpx;
}

.review {
  width: 100%;
  margin-bottom: 8rpx;
}

.review>text:first-child {
  margin: 36rpx 0rpx 16rpx 40rpx;
}

.discuss {
  width: 670rpx;
  margin: 20rpx 40rpx 40rpx 40rpx;
}

.discuss>image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
}

.discuss>view {
  width: 602rpx;
}

.allreview {
  width: 194rpx;
  height: 52rpx;
  border: 2rpx solid rgba(65, 216, 249, 1);
  border-radius: 8rpx;
  margin: 0 auto;
  margin-bottom: 18rpx;
}

.shop-line {
  width: 100%;
  height: 108rpx;
  margin-bottom: 8rpx;
}

.shop-line>image {
  width: 88rpx;
  height: 88rpx;
}

.shop-info {
  width: 335rpx;
  height: 88rpx;
}

.shop-info>text {
  width: 100%;
}

.icon {
  width: 20rpx;
  height: 20rpx;
}

.shops {
  width: 128rpx;
  height: 48rpx;
  line-height: 48rpx;
  background: linear-gradient(270deg, rgba(65, 216, 249, 1) 0%, rgba(31, 175, 241, 1) 100%);
}

.abstract {
  width: 100%;
  margin-bottom: 8rpx;
}

.abstract>view {
  width: 680rpx;
}

.abstract>view:first-child {
  margin-top: 36rpx;
  margin-bottom: 16rpx;
}

.abstract>view:last-child {
  line-height: 44rpx;
}

.traffic {
  width: 100%;
  margin-bottom: 8rpx;
}

