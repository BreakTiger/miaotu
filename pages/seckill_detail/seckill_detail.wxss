}

.attractions {
  width: 100%;
}

.attractions>text {
  width: 680rpx;
  margin-top: 36rpx;
  margin-bottom: 16rpx;
}

.attractions>view {
  width: 680rpx;
}

.back {
  width: 108rpx;
  height: 108rpx;
